import hashlib
file1 = open("c:/Users/john/Documents/oldfile.txt", 'r').read().strip().strip('-').strip().encode('utf-8')
file2 = open("c:/Users/john/Documents/newfile.txt", 'r').read().strip().strip('-').strip().encode('utf-8')
hash1 = hashlib.md5(file1).hexdigest()
hash2 = hashlib.md5(file2).hexdigest()
print(file1)
print(file2)
print(hash1)
print(hash2)
if (hash1 == hash2):
    print('Duplicate file found!')
else:
    print('The files do not match!')
