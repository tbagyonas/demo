a = list(range(1,100))
print(a)
total = 0
for i in a:
    if i % 3 == 0 or i % 5 == 0:
        total += 1
print(total)

given_list3 = [7, 5, 4, 4, 3, 1, -1, -2, -3, -5, -7]
total1 = 0
i = 0
while i < len(given_list3):
    if given_list3[i] < 0:
        total1 += given_list3[i]
    i += 1

print(total1)
